@model EventManagement.Areas.Accounting.ViewModels.BalanceSheetViewModels
<section class="content-header">
    <h1>
        Balance Sheet
        <small>preview of balance sheet</small>
    </h1>
    <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
        <li><a href="#">Balance Sheet</a></li>
    </ol>
</section>

<!-- Main content -->
<section class="content">
    <div class="row">
        <div class="col-md-12">
            <div class="box">
                <!-- /.box-header -->
                <div class="box-body">
                    <table class="table table-bordered">
                        <thead>
                            <tr>
                                <th>Particulars</th>
                                <th>Dirham</th>
                                <th>Dirham</th>
                                <th>Dirham</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model.BalanceSheets)
                            {
                                <tr>
                                    <td>@item.Particulars</td>
                                    <td>@item.ValueOne</td>
                                    <td>@item.ValueTwo</td>
                                    <td>@item.ValueThree</td>
                                </tr>
                            }
                        </tbody>
                    </table>
                </div>
            </div>
            <!-- /.box -->
        </div>
        <!-- /.col -->

    </div>
</section>